<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
    <nav>
        <ul>
            <li><a href="/">Accueil</a> </li>
            <li><a href="{{ path('contact') }}">Contact</a> </li>
            <li><a href="{{ path('contact', {city: 'montpellier'}) }}">Contacter Montpellier</a> </li>
            <li><a href="{{ path('contact', {customerName: 'Rayan'}) }}">Contacter de la part de Rayan</a> </li>
            <li><a href="{{ path('contact', {city: 'montpellier',customerName: 'Rayan'}) }}">Contacter Montpellier de la part de Rayan</a> </li>
        </ul>
    </nav>
        {% block body %}{% endblock %}
    </body>
</html>
